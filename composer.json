{
	"name": "stanislav-janu/gps",
	"description": "GPS extension for nette",
	"homepage": "https://lweb.cz",
	"license": "MIT",
	"authors": [
		{
			"name": "Stanislav JanÅ¯",
			"homepage": "http://lweb.cz"
		}
	],
	"require": {
		"php": ">=7.1.0",
		"nette/utils": "^2.4"
	},
	"require-dev": {
		"stanislav-janu/phpstan": "^0.11",
		"nette/tester": "^2.1"
	},
	"conflict": {
		"nette/nette": "<2.3"
	},
	"scripts": {
		"t": "vendor/bin/tester tests",
		"sa": "vendor/bin/phpstan analyse src -l 7",
		"cs-t": "temp/nette-coding-standard/ecs check src tests --config temp/nette-coding-standard/coding-standard-php71.yml",
		"cs-f": "temp/nette-coding-standard/ecs check src tests --config temp/nette-coding-standard/coding-standard-php71.yml --fix"
	},
	"autoload": {
		"classmap": ["src/"]
	},
	"autoload-dev": {
		"psr-4": {
			"JCode\\Tests\\": "tests"
		}
	},
	"minimum-stability": "stable",
	"extra": {
		"branch-alias": {
			"dev-master": "1.0-dev"
		}
	}
}
